#, c-format
msgid ""
"Processed %.3f MB, at a %.3f MB/sec rate (1 MB == 1000000 Bytes).\n"
"\n"
msgstr ""
"Επεξεργάστηκαν %.3f MB, με ρυθμό %.3f MB/sec (1 MB == 1000000 Bytes).\n"
"\n"

#: src/extra.c:757
#, c-format
msgid ""
"Processed %.3f KB, at a %.3f KB/sec rate (1 KB == 1000 Bytes).\n"
"\n"
msgstr ""
"Επεξεργάστηκαν %.3f KB, με ρυθμό %.3f KB/sec (1 KB == 1000 Bytes).\n"
"\n"

