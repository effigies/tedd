#: src/keys.c:134
msgid "Key transformation failed.\n"
msgstr "Η μετατροπή του κλειδιού απέτυχε.\n"

#: src/xmalloc.c:38 src/xmalloc.c:56 src/xmalloc.c:73 src/xmalloc.c:92
#: src/xmalloc.c:108 src/xmalloc.c:125 src/xmalloc.c:147
